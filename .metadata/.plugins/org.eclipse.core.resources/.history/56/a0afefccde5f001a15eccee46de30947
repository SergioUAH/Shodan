package com.uah.shodan_tfg.entrypoints.dto;

import org.springframework.data.annotation.Id;

import com.uah.shodan_tfg.entities.Host;
import com.uah.shodan_tfg.entities.Location;

public class QueryDTO {

    @Id
    private Integer id;
    
    private Host host;
    
    private Location location;

    private String org;

    public QueryDTO(Integer id, Host host, Location location, String org) {
	super();
	this.id = id;
	this.host = host;
	this.location = location;
	this.org = org;
    }

}
